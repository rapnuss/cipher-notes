{
  "name": "backend",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "build": "NODE_ENV=production bun build src/index.ts --outdir dist --target bun --minify --sourcemap",
    "start": "NODE_ENV=production bun dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.855.0",
    "@aws-sdk/client-ses": "^3.855.0",
    "@aws-sdk/s3-presigned-post": "^3.855.0",
    "@aws-sdk/s3-request-presigner": "^3.855.0",
    "cookie-parser": "^1.4.7",
    "drizzle-orm": "^0.41.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "express-zod-api": "^22.14.1",
    "hcaptcha": "^0.2.0",
    "http-errors": "^2.0.0",
    "jose": "^6.0.12",
    "pg": "^8.16.3",
    "socket.io": "^4.8.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/http-errors": "^2.0.5",
    "@types/pg": "^8.15.5",
    "bun-types": "1.2.7",
    "drizzle-kit": "^0.30.6"
  }
}
